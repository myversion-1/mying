{
  "name": "mying-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "analyze": "cross-env ANALYZE=true next build",
    "lighthouse:test": "powershell -ExecutionPolicy Bypass -File scripts/lighthouse-test-simple.ps1",
    "lighthouse:simple": "powershell -ExecutionPolicy Bypass -File scripts/run-lighthouse-simple.ps1",
    "lighthouse:quick": "powershell -ExecutionPolicy Bypass -File scripts/lighthouse-quick.ps1",
    "kill:nextjs": "powershell -ExecutionPolicy Bypass -File scripts/kill-nextjs.ps1",
    "lighthouse:compare": "powershell -ExecutionPolicy Bypass -File scripts/generate-comparison.ps1",
    "lighthouse:before": "lighthouse http://localhost:3000/products --output=json --output-path=./lighthouse-reports/before.json --chrome-flags=\"--headless\"",
    "lighthouse:after": "lighthouse http://localhost:3000/products --output=json --output-path=./lighthouse-reports/after.json --chrome-flags=\"--headless\"",
    "perf:monitor": "powershell -ExecutionPolicy Bypass -File scripts/performance-monitor.ps1",
    "verify:compression": "powershell -ExecutionPolicy Bypass -File scripts/verify-compression.ps1",
    "clean": "powershell -ExecutionPolicy Bypass -File scripts/clean-build.ps1",
    "diagnose:404": "powershell -ExecutionPolicy Bypass -File scripts/diagnose-404.ps1",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@microsoft/clarity": "^1.0.2",
    "@sendgrid/mail": "^8.1.3",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "critters": "^0.0.23",
    "lucide-react": "^0.469.0",
    "next": "16.1.1",
    "next-intl": "^4.7.0",
    "next-seo": "^7.0.1",
    "nodemailer": "^7.0.12",
    "react": "19.2.3",
    "react-countup": "^6.5.2",
    "react-dom": "19.2.3",
    "resend": "^4.0.0",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.1.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
